<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <ul>
    <% renderedContent.forEach(book=> { %>
      <li>img src="<%= book.image_url %>" alt="<%= book.title %>">
            <p id="title">
              <%= book.title %>
            </p>
            <p id="title">
              <%= book.author %>
            </p>
            <p>
              <%= book.description %>
            </p>
            <button><a href="/books/<%= book.id %>">View Details</a></button></li>
      <% }) %>

  </ul>
  
</body>
</html>